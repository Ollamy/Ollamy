{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "Yoel Edery",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "ci:lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --max-warnings=0",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "build:openapi-generator-cli": "openapi-generator-cli generate -g typescript-nestjs --additional-properties=useSingleRequestParameter=true,withoutRuntimeChecks=true,typescriptThreePlus=true,modelPropertyNaming=original --skip-validate-spec -g typescript-fetch --enable-post-process-file --remove-operation-id-prefix  -o out -i ./swagger.json",
    "prisma:generate": "yarn prisma generate",
    "prisma:migrate": "yarn prisma migrate dev"
  },
  "dependencies": {
    "@google-cloud/vertexai": "^1.1.0",
    "@nestjs/common": "^9.4.0",
    "@nestjs/core": "^9.4.0",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/platform-socket.io": "^10.2.4",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/testing": "^9.4.2",
    "@nestjs/websockets": "^10.2.4",
    "@ollamy/utils": "*",
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "@prisma/client": "latest",
    "@types/cookie-parser": "^1.4.3",
    "@types/crypto-js": "^4.1.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mime-types": "^2.1.4",
    "@types/socket.io": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.3.1",
    "env-var": "^7.3.0",
    "env-variable": "^0.0.6",
    "jsonwebtoken": "^9.0.0",
    "mime-types": "^2.1.35",
    "pbkdf2": "^3.1.2",
    "pg": "^8.11.3",
    "redis": "^4.6.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "rxjs": "^7.8.0",
    "webpack": "^5.82.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.4.0",
    "@nestjs/schematics": "^9.1.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/multer": "^1.4.11",
    "@types/node": "^18.14.1",
    "@types/supertest": "^2.0.12",
    "jest": "^29.4.3",
    "jest-junit": "^16.0.0",
    "prettier": "^2.8.4",
    "prisma": "5.15.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.14.2",
    "typescript": "^4.9.5",
    "yarn-upgrade-all": "^0.7.2"
  }
}
