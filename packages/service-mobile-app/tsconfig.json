{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "composite": true,
    "moduleSuffixes": [
      ".native",
      ".android",
      ".ios",
      ""
    ],
    "baseUrl": "./src",
    "paths": {
      "@ollamy/design-system/*": [
        "../util-design-system/*"
      ],
      "@ollamy/utils/*": [
        "../util-utils/*"
      ]
    }
  },
  "extends": "expo/tsconfig.base"
}
